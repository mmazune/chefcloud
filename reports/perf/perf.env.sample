# Copy to .perf.env and adjust for your dev DB
# WARNING: NEVER point at production! This runs EXPLAIN ANALYZE which executes queries.

DATABASE_URL=postgresql://postgres:postgres@localhost:5432/chefcloud_dev
PRISMA_LOG_QUERIES=true
EXPLAIN_FORMAT=text   # or json

# Test org/branch for queries (must exist in dev DB)
ORG_ID=ORG_TEST
BRANCH_ID=BRANCH_TEST

# Period for time-based queries
PERF_PERIOD=2024-11
